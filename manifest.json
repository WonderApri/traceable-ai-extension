{
  "manifest_version": 3,
  "name": "TraceableAI: Deepfake Auditor",
  "version": "0.1",
  "description": "Verify media authenticity in real-time.",
  "permissions": ["storage", "tabs"],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*", "https://x.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": ["<all_urls>"]

}